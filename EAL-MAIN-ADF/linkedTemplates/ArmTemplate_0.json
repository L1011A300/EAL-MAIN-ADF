{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "EAL-MAIN-ADF"
		},
		"MIS_Choice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MIS_Choice'"
		},
		"Radixx_FTP_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Radixx_FTP'"
		},
		"SharePointOnlineList1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'SharePointOnlineList1'"
		},
		"SkedFlexConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SkedFlexConnection'"
		},
		"eal_edw_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eal_edw_connection'"
		},
		"eal_main_blob_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eal_main_blob_connection'"
		},
		"eal_staging_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eal_staging_connection'"
		},
		"MIS_Choice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.airline-choice.com/rest/"
		},
		"MIS_Choice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eastern-test"
		},
		"Radixx_FTP_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "66.35.82.100"
		},
		"Radixx_FTP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ealftp01"
		},
		"SharePointOnlineList1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "9269a1f1-59ff-4866-97bc-78de45083f85"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/MIS_Choice')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "OData",
				"typeProperties": {
					"url": "[parameters('MIS_Choice_properties_typeProperties_url')]",
					"authenticationType": "Basic",
					"userName": "[parameters('MIS_Choice_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('MIS_Choice_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Radixx_FTP')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "FTP server for Radixx TCN source files",
				"annotations": [],
				"type": "FtpServer",
				"typeProperties": {
					"host": "[parameters('Radixx_FTP_properties_typeProperties_host')]",
					"port": "990",
					"enableSsl": true,
					"enableServerCertificateValidation": true,
					"authenticationType": "Basic",
					"userName": "[parameters('Radixx_FTP_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Radixx_FTP_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/SharePointOnlineList1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SharePointOnlineList",
				"typeProperties": {
					"siteUrl": "https://dyaops.sharepoint.com/sites/EAL_OPS_REPORTS",
					"tenantId": "81cbafb6-8886-400c-826d-4293940c3d19",
					"servicePrincipalId": "[parameters('SharePointOnlineList1_properties_typeProperties_servicePrincipalId')]",
					"servicePrincipalKey": {
						"type": "SecureString",
						"value": "[parameters('SharePointOnlineList1_servicePrincipalKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/SkedFlexConnection')]",

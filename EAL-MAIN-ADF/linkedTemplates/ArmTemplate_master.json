{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "EAL-MAIN-ADF"
		},
		"SharePointOnlineList1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'SharePointOnlineList1'"
		},
		"SkedFlexConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SkedFlexConnection'"
		},
		"eal_edw_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eal_edw_connection'"
		},
		"eal_main_blob_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eal_main_blob_connection'"
		},
		"eal_staging_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eal_staging_connection'"
		},
		"SharePointOnlineList1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "azasqadmin-d"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "EAL-MAIN-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SharePointOnlineList1_servicePrincipalKey": {
						"value": "[parameters('SharePointOnlineList1_servicePrincipalKey')]"
					},
					"SkedFlexConnection_connectionString": {
						"value": "[parameters('SkedFlexConnection_connectionString')]"
					},
					"eal_edw_connection_connectionString": {
						"value": "[parameters('eal_edw_connection_connectionString')]"
					},
					"eal_main_blob_connection_connectionString": {
						"value": "[parameters('eal_main_blob_connection_connectionString')]"
					},
					"eal_staging_connection_connectionString": {
						"value": "[parameters('eal_staging_connection_connectionString')]"
					},
					"SharePointOnlineList1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('SharePointOnlineList1_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "EAL-MAIN-ADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'EAL-MAIN-ADF_ArmTemplate_0')]"
			]
		},
		{
			"name": "EAL-MAIN-ADF_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'EAL-MAIN-ADF_ArmTemplate_1')]"
			]
		},
		{
			"name": "EAL-MAIN-ADF_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'EAL-MAIN-ADF_ArmTemplate_2')]"
			]
		}
	]
}
{
	"name": "Parse NavBlue RecallFlight XML",
	"properties": {
		"folder": {
			"name": "NavBlue"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "NAVBLUE_CSV_RecallFlight",
						"type": "DatasetReference"
					},
					"name": "NavBlueCSVreallyXML"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ParseXML"
				},
				{
					"name": "flatten1"
				},
				{
					"name": "stringify1"
				},
				{
					"name": "RemoveXMLcolumn"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_col0_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NavBlueCSVreallyXML",
				"NavBlueCSVreallyXML parse(New = {_col0_} ? (FlightPlan as (FlightPlanSummary as (OutTime as (EstimatedTime as (Value as string))))),",
				"          EstFuelWeight = {_col0_} ? (FlightPlan as (FuelHeader as (TakeOffFuel as (Duration as (Value as string))))),",
				"     format: 'xml',",
				"     namespaces: false) ~> ParseXML",
				"NavBlueCSVreallyXML foldDown(unroll(),",
				"     mapColumn(",
				"          Data",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"NavBlueCSVreallyXML stringify(XML = {_col0_} ? string,",
				"     format: 'delimited',",
				"     columnNamesAsHeader: true,",
				"     columnDelimiter: '     ',",
				"     nullValue: '') ~> stringify1",
				"ParseXML select(mapColumn(",
				"          New,",
				"          EstFuelWeight",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveXMLcolumn"
			]
		}
	}
}
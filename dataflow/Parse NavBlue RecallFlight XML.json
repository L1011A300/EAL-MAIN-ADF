{
	"name": "Parse NavBlue RecallFlight XML",
	"properties": {
		"folder": {
			"name": "NavBlue"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "NAVBLUE_CSV_RecallFlight",
						"type": "DatasetReference"
					},
					"name": "NavBlueCSVreallyXML"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ParseXML"
				},
				{
					"name": "stringify1"
				},
				{
					"name": "RemoveXMLcolumn"
				},
				{
					"name": "flatten2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {_col0_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> NavBlueCSVreallyXML",
				"NavBlueCSVreallyXML parse(EstOutTime = {_col0_} ? (FlightPlan as (FlightPlanSummary as (OutTime as (EstimatedTime as (Value as string))))),",
				"          TakeOffFuelDuration = {_col0_} ? (FlightPlan as (FuelHeader as (TakeOffFuel as (Duration as (Value as string))))),",
				"          TakeOffFuelWeightUnit = {_col0_} ? (FlightPlan as (FuelHeader as (TakeOffFuel as (EstimatedWeight as ({@unit} as string))))),",
				"          TakeOffFuelWeightKG = {_col0_} ? (FlightPlan as (FuelHeader as (TakeOffFuel as (EstimatedWeight as string)))),",
				"     format: 'xml',",
				"     namespaces: false) ~> ParseXML",
				"NavBlueCSVreallyXML stringify({_col0_} = {_col0_} ? string,",
				"     format: 'delimited',",
				"     columnNamesAsHeader: true,",
				"     columnDelimiter: '     ',",
				"     nullValue: '') ~> stringify1",
				"ParseXML select(mapColumn(",
				"          EstOutTime,",
				"          TakeOffFuelDuration,",
				"          TakeOffFuelWeight = TakeOffFuelWeightUnit",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveXMLcolumn",
				"NavBlueCSVreallyXML foldDown(unroll(),",
				"     mapColumn(",
				"          {_col0_}",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten2"
			]
		}
	}
}
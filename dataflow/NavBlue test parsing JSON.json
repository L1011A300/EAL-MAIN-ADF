{
	"name": "NavBlue test parsing JSON",
	"properties": {
		"folder": {
			"name": "TEST"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "NAVBLUE_JSON",
						"type": "DatasetReference"
					},
					"name": "NavBlueJSON"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "parse1"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> NavBlueJSON",
				"flatten1 parse(column1 = ERROR_FUNCTION(''),",
				"     format: 'xml',",
				"     namespaces: true) ~> parse1",
				"NavBlueJSON foldDown(unroll(),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}